#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules/ts-node/dist/node_modules:/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules/ts-node/node_modules:/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules:/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules/ts-node/dist/node_modules:/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules/ts-node/node_modules:/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules:/mnt/c/Users/HP/orbit-nuel-/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules/ts-node/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-node@10.9.2_@swc+core@1._b9032bef52725834e2157c828e1d1b38/node_modules/ts-node/dist/bin-esm.js" "$@"
fi
